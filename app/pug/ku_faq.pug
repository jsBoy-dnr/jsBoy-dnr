doctype html
html(class="no-js", lang="en")

    include ../components/head/head
    include ../components/header_top/header_top_custom
    include ../components/header_right/header_right
    include ../components/header_notif/header_notif
    include ../components/breadcrumbs/breadcrumbs
    include ../components/section_title/section_title
    include ../components/section_title/title_bl
    include ../components/faq_block/faq_block
    include ../components/faq_block/faq_filter
    include ../components/footer/footer_custom

    head
        +head
        link(rel='stylesheet', href='../css/header_top/header_top.css')
        link(rel='stylesheet', href='../css/header_right/header_right.css')
        link(rel='stylesheet', href='../css/header_notif/header_notif.css')
        link(rel='stylesheet', href='../css/header_content/header_content.css')
        link(rel='stylesheet', href='../css/header_sub_menu/header_sub_menu.css')
        link(rel='stylesheet', href='../css/kun_template/kun_template.css')
        link(rel='stylesheet', href='../css/breadcrumbs/breadcrumbs.css')
        link(rel='stylesheet', href='../css/faq_block/faq_block.css')
        link(rel='stylesheet', href='../css/footer/footer.css')
    body
        .site-wrapper.template--aim.ku.faq
            header.site-header.with-bg.small
                +header_top_custom
                +header_right
            #header_content.header_content--menu
                nav.header-sub-menu-wrap
                    ul.header-sub-menu
                        li
                            a(href="#") Корпоративный Универститет
                        li.-drop-
                            a(href="#") О нас
                            ul
                                li
                                    a(href="#") Наши цели и задачи
                                li
                                    a(href="#") Наша команда
                        li.-drop-
                            a(href="#") Каталог программ
                            ul
                                li
                                    a(href="#") Факультет менеджмента и лидерства
                                li
                                    a(href="#") Факультет личной эффективности
                                li.-dropright-
                                    a(href="#") Факультет по функциям
                                    ul
                                        li
                                            a(href="#") Факультет HR
                                        li
                                            a(href="#") Факультет закупок
                                        li
                                            a(href="#") Факультет проектного управления
                                li
                                    a(href="#") Факультет цифровых навыков и цифровизации
                                li
                                    a(href="#") Факультет английского языка
                                li
                                    a(href="#") Адаптация в компании и в роли

                        li
                            a(href="#") Корпоративная библиотека

                        li.-drop-
                            a(href="#") Банк знаний
                            ul
                                li
                                    a(href="#") Записи вебинаров
                                li
                                    a(href="#") Образовательная рассылка
                                li
                                    a(href="#") Нормативные документы
                        li
                            a(href="#") Нормативные документы
                        li
                            a(href="#") FAQ
            .blocks-wrapper
                .inner
                    +breadcrumbs('Корпоративный университет', 'Вопрос-ответ')
                    section.section.faq__wrap
                        +section_title('titles/qa_title.png')
                        +faq_block

        +footer_custom

        script(src='../js/separate-js/svg4everybody.min.js')
        script svg4everybody();

        script(src='../js/separate-js/jquery-2.2.4.min.js')
        script(src='../js/separate-js/owl.carousel.min.js')
        script(src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js")
        script(src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js")
        script(src='../js/compiled.js')
        script(src='../components/faq_block/faq_block.js')
        script(src='../components/footer/footer.js')
